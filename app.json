    },
    "android": {
      "package": "com.vmplg.planetleague",
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/stickers/generic_sticker.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./src/assets/stickers/generic_sticker.png"
    },
    "plugins": [
      "expo-font",
      "expo-web-browser"
    ],
    "extra": {
      "eas": {
        "projectId": "94272f9f-33fa-4d06-996d-19b636229768"
      }
    },
    "owner": "vmplg"
  }
