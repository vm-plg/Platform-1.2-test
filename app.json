{
  "expo": {
    "name": "Planet League",
    "slug": "planet-league",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/stickers/generic_sticker.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/stickers/generic_sticker.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
